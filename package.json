{
  "name": "naschi.at",
  "version": "0.3.0",
  "private": false,
  "description": "My personal Website",
  "author": "Daniel Naschberger <daniel@naschi.at>",
  "license": "GPL-3.0",
  "scripts": {
    "dev": "astro dev",
    "lint": "eslint 'src/**/*.{astro,js,ts,vue}' --fix",
    "lint-no-fix": "eslint 'src/**/*.{js,ts,vue}'",
    "build": "astro build",
    "preview": "astro preview",
    "optimize-images:1280": "./image-optimizer --source ./media/ --destination public/media --suffix 1280 --width 1280 --quality 90 --webpimage true --thumbnail false",
    "optimize-images:768": "./image-optimizer --source ./media/ --destination public/media --suffix 768 --width 768 --quality 90 --webpimage true --thumbnail false",
    "optimize-images:640": "./image-optimizer --source ./media/ --destination public/media --suffix 640 --width 640 --quality 90 --webpimage true --thumbnail false",
    "optimize-images": "rm -rf public/media && yarn optimize-images:1280 && yarn optimize-images:768 && yarn optimize-images:640"


  },
  "dependencies": {
    "astro": "^1.0.0-rc.3",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@astrojs/vue": "^0.5.0",
    "@types/node": "^18.6.3",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-plugin-typescript": "^5.0.8",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "eslint": "^8.21.0",
    "eslint-plugin-astro": "^0.16.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.3.0",
    "prettier": "^2.7.1",
    "prettier-eslint": "^15.0.1",
    "prettier-plugin-astro": "^0.2.0",
    "typescript": "^4.7.4"
  }
}
